{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node ace dist --production",
    "start": "node server.js",
    "dev": "node ace serve --watch",
    "lint": "yarn eslint app start --ext ts,js,tsx,jsx --fix",
    "test": "node -r @adonisjs/assembler/build/register japaFile.ts",
    "test:watch": "nodemon -e ts --exec 'yarn test'",
    "coverage": "nyc yarn test",
    "clean": "rimraf dist *.tsbuildinfo"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.0.0",
    "@types/supertest": "^2.0.11",
    "@types/node-fetch": "^2.5.10",
    "@types/redis": "^2.8.29",
    "@types/socket.io": "^3.0.2",
    "adonis-preset-ts": "^2.1.0",
    "execa": "^5.0.1",
    "get-port": "^5.1.1",
    "japa": "^3.1.1",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "pino-pretty": "^4.8.0",
    "supertest": "^6.1.3",
    "typescript": "~4.2",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/core": "^5.1.0",
    "@adonisjs/redis": "^7.0.4",
    "@adonisjs/repl": "^3.0.0",
    "@musicroom/types": "*",
    "@socket.io/redis-adapter": "^7.0.0",
    "googleapis": "^74.1.0",
    "node-fetch": "^2.6.1",
    "proxy-addr": "^2.0.6",
    "query-string": "^7.0.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.1.2",
    "source-map-support": "^0.5.19",
    "zod": "^1.11.17"
  },
  "workspaces": {
    "nohoist": [
      "*adonis-preset-ts*",
      "*adonis-preset-ts*/**"
    ]
  },
  "scripty": {
    "path": "../../scripts/packages"
  }
}
